          },
          {
            "name": "pagesize",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "site",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "default": "stackoverflow"
