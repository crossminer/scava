{
	"metricid" : "org.eclipse.scava.metricprovider.trans.rascal.CC.java.CCHistogramJava.historic",
	"vis" : [ 
		{
			"id" : "CCHistogramJavaTimeLine",
			"name" : "Number of Java methods per Cyclomatic Complexity risk factor",
			"description" : "This metric shows the number of Java methods per CC risk factor over time.",
			"type" : "LineChart",
			"datatable" : {
				"rows" : "$value",
				"cols" : [  { "name" : "Date", 			"field" : "$__date" }, 
							{ "name" : "Factor", 		"field" : "$value[0].value" }, 
							{ "name" : "Count", 		"field" : "$value[1].value" } ]
			},
			"x" : "Date",
			"y" : "Count", 
			"series" : "Factor",
			"x-label" : "Date",		
			"y-label" : "Number of methods",
			"timeSeries" : true
		}
	]
}
