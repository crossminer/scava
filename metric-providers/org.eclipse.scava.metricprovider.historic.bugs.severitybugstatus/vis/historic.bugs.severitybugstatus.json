			"id" : "bugs.severity.resolvedClosedBugs",
			"name" : "Resolved or Closed Bug Severity",
			"description" : "The number of resolved or closed bugs per severity level",
			"type" : "LineChart",
			"datatable" : {
				"rows" : "$severityLevels",
				"cols" : [  { "name" : "Date", "field" : "$__date" }, 
					{ "name" : "Severity", "field" : "$severityLevel" },
					{ "name" : "Bugs", "field" : "$numberOfResolvedClosedBugs" } ]
			},
