package org.eclipse.crossmeter.metricprovider.historic.bugs.severity.model;

class BugsSeveritiesHistoricMetric {
	val BugData[*] bugData;
	val SeverityLevel[*] severityLevels;
}

class BugData {
	attr String bugTrackerId;
	attr int numberOfBugs;
}


class SeverityLevel {
	attr String bugTrackerId;
	attr String severityLevel;
	attr int numberOfBugs;
	attr float percentage;
}

